class Solution {
    public int majorityElement(int[] nums) {
        int k = nums.length/2;
        HashMap<Integer, Integer>map = new HashMap<>();
        for(int i=0;i<nums.length;i++){
            int num = nums[i];
        int oldcount = map.getOrDefault(num,0);
        int newcount = oldcount + 1;
        map.put(num,newcount);
        //it interrates one by one and upadate the value and again cheks for this
        if(newcount>k){
            return num;
        }
        }
        return -1;  
    }
}
