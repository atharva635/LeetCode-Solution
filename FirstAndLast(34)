class Solution {
    public int[] searchRange(int[] nums, int target) {
        int first = fisoc(nums,target);
        int last = lasoc(nums,target);
        return new int[]{first,last};
    }
    public int fisoc(int []nums,int target){
        int start = 0,ans = -1;
        int end = nums.length-1;
        while(start<=end){
            int mid = start + (end - start)/2;
            if(nums[mid]==target){
                ans = mid;
                //agar mid rigth side aagya and fist occurence me left side bhi 
                //ek target ho to uske liye end = mid - 1;
                end = mid - 1;
            }
            else if(nums[mid]>target){
                end = mid -1;
            }
            else{
                start = mid + 1;
            }
        }
        return ans;   
    }

    public int lasoc(int []nums,int target){
        int start = 0,ans = -1;
        int end = nums.length-1;
        while(start<=end){
            int mid = start + (end - start)/2;
            if(nums[mid]==target){
                ans = mid;
                start = mid + 1;
            }
            else if(nums[mid]<target){
                start = mid +1;
            }
            else{
                end = mid - 1;
            }
        }
        return ans;   
    }
}
